{
	"name": "github-insights",
	"version": "1.0.0",
	"description": "Get insights for your Github repositories",
	"main": "api/index.js",
	"scripts": {
		"fetch": "nodemon api/index.js",
		"start": "parcel serve app/index.html",
		"build": "parcel build app/index.html --public-url '/'"
	},
	"keywords": [
		"Github insights",
		"Insights",
		"Data Vis"
	],
	"repository": "git@github.com:scriptex/github-insights.git",
	"bugs": {
		"url": "https://github.com/scriptex/github-insights/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"license": "MIT",
	"dependencies": {
		"chalk": "4.1.2",
		"date-fns": "2.24.0",
		"dotenv": "10.0.0",
		"express": "4.17.1",
		"node-fetch": "3.0.0",
		"nodemon": "2.0.12",
		"ora": "6.0.1",
		"prop-types": "15.7.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-is": "17.0.2",
		"recharts": "2.1.4",
		"scriptex-socials": "1.4.0",
		"styled-components": "5.3.1",
		"universal-github-client": "1.0.3",
		"yargs": "17.1.1"
	},
	"devDependencies": {
		"@babel/core": "7.15.5",
		"@babel/plugin-transform-runtime": "7.15.0",
		"@babel/preset-env": "7.15.6",
		"@babel/preset-react": "7.14.5",
		"parcel-bundler": "1.12.5"
	},
	"nodemonConfig": {
		"verbose": false,
		"ignore": [
			"insights.json"
		]
	}
}
