{
	"name": "github-insights",
	"version": "1.0.0",
	"description": "Get insights for your Github repositories",
	"scripts": {
		"fetch": "nodemon api/index.js",
		"start": "parcel serve app/index.html",
		"build": "rm -rf dist && parcel build app/index.html --public-url '/'"
	},
	"keywords": [
		"Github insights",
		"Insights",
		"Data Vis"
	],
	"repository": "git@github.com:scriptex/github-insights.git",
	"bugs": {
		"url": "https://github.com/scriptex/github-insights/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"license": "MIT",
	"dependencies": {
		"chalk": "4.1.2",
		"date-fns": "2.28.0",
		"dotenv": "16.0.0",
		"express": "4.17.2",
		"node-fetch": "2.6.7",
		"nodemon": "2.0.15",
		"ora": "5.4.1",
		"prop-types": "15.8.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-is": "17.0.2",
		"recharts": "2.1.9",
		"scriptex-socials": "1.5.1",
		"styled-components": "5.3.3",
		"universal-github-client": "1.0.3",
		"yargs": "17.3.1"
	},
	"devDependencies": {
		"@babel/core": "7.17.3",
		"@babel/plugin-transform-runtime": "7.17.0",
		"@babel/preset-env": "7.16.11",
		"@babel/preset-react": "7.16.7",
		"parcel": "2.3.0"
	},
	"nodemonConfig": {
		"verbose": false,
		"ignore": [
			"insights.json"
		]
	}
}
