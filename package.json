{
	"name": "github-insights",
	"version": "1.0.0",
	"description": "Get insights for your Github repositories",
	"main": "api/index.js",
	"scripts": {
		"fetch": "nodemon api/index.js",
		"start": "parcel app/index.html",
		"build": "parcel build app/index.html --public-url '/'"
	},
	"keywords": [
		"Github insights",
		"Insights",
		"Data Vis"
	],
	"repository": "git@github.com:scriptex/github-insights.git",
	"bugs": {
		"url": "https://github.com/scriptex/github-insights/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"license": "MIT",
	"dependencies": {
		"chalk": "4.1.0",
		"date-fns": "2.15.0",
		"dotenv": "8.2.0",
		"express": "4.17.1",
		"node-fetch": "2.6.0",
		"nodemon": "2.0.4",
		"ora": "5.0.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"recharts": "1.8.5",
		"universal-github-client": "1.0.3",
		"yargs": "15.4.1"
	},
	"devDependencies": {
		"@babel/core": "7.11.1",
		"@babel/plugin-transform-runtime": "7.11.0",
		"@babel/preset-env": "7.11.0",
		"@babel/preset-react": "7.10.4",
		"parcel-bundler": "1.12.4"
	},
	"nodemonConfig": {
		"verbose": false,
		"ignore": [
			"insights.json"
		]
	}
}
