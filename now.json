{
	"version": 2,
	"alias": "github-repository-insights",
	"builds": [
		{ "src": "api/*.js", "use": "@now/node" },
		{ "src": "lib/*.js", "use": "@now/node" },
		{ "src": "app/*.*", "use": "@now/static" }
	],
	"routes": [
		{
			"src": "/insights",
			"dest": "/api/insights.js",
			"methods": ["POST"]
		}
	]
}
